    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "SpringSecurityClientApplication",
            "request": "launch",
            "mainClass": "com.yash.client.SpringSecurityClientApplication",
            "projectName": "spring-security-app"
        },
        {
            "type": "java",
            "name": "Spring Boot-SpringSecurityClientApplication<spring-security-client>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.yash.client.SpringSecurityClientApplication",
            "projectName": "spring-security-client",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}